<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="scip.app.CouplesMain"
    android:id="@+id/Couple_Main_Screen">

    <ImageButton
        android:layout_width="400dp"
        android:layout_height="450dp"
        android:layout_x="16dp"
        android:id="@+id/imageButton"
        android:layout_gravity="center"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="-20dp"
        android:background="@color/material_blue_500"
        android:paddingLeft="50dp"
        android:paddingRight="50dp"
        android:paddingTop="50dp"
        android:paddingBottom="50dp"
        android:scaleType="fitStart"
        android:src="@drawable/ic_cycle" />

    <ImageButton
        android:layout_width="400dp"
        android:layout_height="450dp"
        android:id="@+id/imageButton3"
        android:layout_gravity="right|center_vertical"
        android:layout_marginRight="16dp"
        android:layout_marginTop="-20dp"
        android:paddingLeft="120dp"
        android:paddingRight="70dp"
        android:paddingTop="100dp"
        android:paddingBottom="60dp"
        android:scaleType="fitStart"
        android:background="@color/material_blue_500"
        android:src="@drawable/ic_cap" />

    <ImageButton
        android:layout_width="400dp"
        android:layout_height="450dp"
        android:id="@+id/imageButton2"
        android:layout_gravity="left|center_vertical"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="-20dp"
        android:background="@color/material_blue_500"
        android:paddingLeft="50dp"
        android:paddingRight="50dp"
        android:paddingTop="30dp"
        android:paddingBottom="50dp"
        android:scaleType="fitStart"
        android:src="@drawable/ic_calendar" />

    <TextView
        android:layout_width="165dp"
        android:layout_height="32dp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Next Peak Fertility : "
        android:id="@+id/PeriodCycleLabel"
        android:layout_gravity="bottom|left|top"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="560dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Next Cycle : "
        android:id="@+id/NextCycleLabel"
        android:layout_gravity="bottom|left|top"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="590dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Calendar View"
        android:id="@+id/textView3"
        android:layout_gravity="bottom|left|top"
        android:layout_marginLeft="140dp"
        android:textSize="@dimen/abc_text_size_headline_material"
        android:layout_marginTop="480dp"
        android:textColor="#ffffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Cycle View"
        android:id="@+id/textView4"
        android:layout_gravity="center_horizontal|bottom|top"
        android:layout_marginTop="480dp"
        android:textSize="@dimen/abc_text_size_headline_material"
        android:textColor="#ffffff" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="ART View"
        android:id="@+id/textView5"
        android:layout_gravity="bottom|right|top"
        android:layout_marginRight="140dp"
        android:layout_marginTop="480dp"
        android:textColor="#ffffff"
        android:textSize="@dimen/abc_text_size_headline_material" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/Peak_Fertility"
        android:layout_gravity="left|top"
        android:layout_marginTop="560dp"
        android:layout_marginLeft="180dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/Next_Cycle"
        android:layout_gravity="left|top"
        android:layout_marginTop="590dp"
        android:layout_marginLeft="120dp" />

</FrameLayout>

